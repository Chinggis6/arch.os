export qutecf=~/.config/qutebrowser
export qutebrowser=$qutecf/qutebrowser.conf
export qute=$qutebrowser
export urls=$qutecf/bookmarks/urls
export keys=$qutecf/keys.conf

export qutelocal=~/.local/share/qutebrowser
export qutebrowser_history=$qutelocal/history
export qutebrowser_cmd_history=$qutelocal/cmd-history

export userscripts=$share/qutebrowser/userscripts
